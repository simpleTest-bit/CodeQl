            });
            $(document).ready(function () {
                $('.sapid-error-msg1').hide();
                $('.sapID1').on('focusout', function () {
                    v = parseInt($(this).val());
                    min = parseInt($(this).attr('min'));
                    max = parseInt($(this).attr('max'));

                    if (v < min) {
                        $(this).val(null);
                        $('.sapid-error-msg1').show();
                    } else if (v > max) {
                        $(this).val(null);
                        $('.sapid-error-msg1').show();
                    }
                    else {
                        $('.sapid-error-msg1').hide();
                    }
                })
            });
